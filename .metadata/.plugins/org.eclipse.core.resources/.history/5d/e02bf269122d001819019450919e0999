package com.cn;

public class Test123 {

	public static void main(String[] args) {

		Animal an=new Animal();
		an.setName("张山");
		an.setAge(30);
		an.show();
	}
}
 class Animal{
	 
	private  String name;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	private int age;
	public void show(){
		
		System.out.println("名字："+name+",年龄"+age);
	}
}