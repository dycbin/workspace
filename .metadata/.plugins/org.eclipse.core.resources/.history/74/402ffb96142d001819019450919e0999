package com.cn;

public class Test123 {

	public static void main(String[] args) {

		Animal an=new Animal();
		Animal an1=new Animal("李四",50);
		System.out.println("===========wucan================");
		System.out.println("姓名"+an.getName());
		System.out.println("年龄"+an.getAge());
		System.out.println("===========youcan================");
		System.out.println("姓名"+an1.getName());
		System.out.println("年龄"+an1.getAge());
	}
}
 class Animal{
	 
	private  String name;
	private  int age;
	private  String color;
	
	public Animal() {
		this.name ="张三";
		this.age =40;
	}
	public Animal(String name) {
		this.name ="王五";
	}
	
	public Animal(String name,int age) {
		this.name =name;
		this.age =age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public String getColor() {
		return color;
	}
	
}