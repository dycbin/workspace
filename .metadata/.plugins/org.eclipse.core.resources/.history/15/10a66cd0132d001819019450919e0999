}
 class Animal{
	 
	private  String name;
	private  int age;
	private  String color;
	
	public Animal() {
		this.name ="张三";
		this.age =40;
	}
	public Animal(String name) {
		this.name ="王五";
	}
	
	public Animal(String name,int age) {
		this.name =name;
		this.age =age;
	}
}