package com.cn;

public class Test123 {

	public static void main(String[] args) {

		Animal an=new Animal("zhangshan");
		System.out.println(an.getname());
	}
}
 class Animal{
	 
	 String name;
	 
	 Animal(String name){
		 System.out.println("mingz"+name);
	 }
	 public String getname(){
		 
		 return name;
	 }
}