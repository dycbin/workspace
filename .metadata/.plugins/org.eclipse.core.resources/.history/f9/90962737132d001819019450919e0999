package com.cn;

public class Test123 {

	public static void main(String[] args) {

		Animal an=new Animal();
		an.setName("张山");
		an.setAge(30);
		an.show();
	}
}
 class Animal{
	 
	private  String name;
	private  int age;
	private  String color;
	
	public Animal() {
		this.name ="张三";
		this.age =40;
	}
	public Animal(String name) {
		this.name ="王五";
	}
	
	public Animal(String name,int age) {
		this.name ="李四";
		this.age =50;
	}
}